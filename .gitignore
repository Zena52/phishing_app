$gi = @'
# Python cache
__pycache__/
*.pyc

# Logs and data
*.csv
phish_ai_logs/

# Streamlit temporary files
.streamlit/
'@
Set-Content -Path .gitignore -Value $gi -Encoding UTF8

# ignore generated summaries
phish_ai_summary.csv

# ignore local AI logs
app/phish_ai_logs/

# ignore logs
Data/phish_ai_logs/*
Data/*.csv
app/*.csv
phish_ai_logs/*
